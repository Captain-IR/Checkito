<?xml version='1.0' encoding='utf-8'?>
<widget id="com.checkitodo.quasar" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Checkitodo</name>
    <description>A Quasar Framework app</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icons/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icons/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icons/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icons/icon-96-xhdpi.png" />
        <splash density="land-hdpi" src="res/splash/splash.9.png" />
        <splash density="land-ldpi" src="res/splash/splash.9.png" />
        <splash density="land-mdpi" src="res/splash/splash.9.png" />
        <splash density="land-xhdpi" src="res/splash/splash.9.png" />
        <splash density="land-xxhdpi" src="res/splash/splash.9.png" />
        <splash density="port-hdpi" src="res/splash/splash.9.png" />
        <splash density="port-ldpi" src="res/splash/splash.9.png" />
        <splash density="port-mdpi" src="res/splash/splash.9.png" />
        <splash density="port-xhdpi" src="res/splash/splash.9.png" />
        <splash density="port-xxhdpi" src="res/splash/splash.9.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
